{
  "compilerOptions": {
    "target": "es6", // 기존 설정 유지
    "module": "esnext", // 기존 설정 유지
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "jsx": "react-jsx",
    "moduleResolution": "node" // 추가된 설정
  },
  "include": ["src/**/*", "vite-env.d.ts"], // 새 파일 포함
  "exclude": ["node_modules"]
}
